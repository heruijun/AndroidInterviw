<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="interview.heruijun.com.androidinterview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NextActivity">
            <intent-filter>
                <action android:name="com.interview.next" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DispatcherViewActivity" />
        <activity
            android:name=".webview.BrowserActivity"
            android:process=":com.inverview.browser" />
        <activity android:name=".webview.WebviewActivity" />
        <activity
            android:name=".normalwebview.NormalwebviewActivity"
            android:label="@string/title_activity_normalwebview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.DispatcherExampleActivity" />
        <activity android:name=".service.ServiceActivity" />

        <service
            android:name=".service.ServiceExample"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".classloader.ClassloaderActivity"></activity>
    </application>

</manifest>